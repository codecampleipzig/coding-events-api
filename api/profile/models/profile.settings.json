{
  "kind": "collectionType",
  "collectionName": "profiles",
  "info": {
    "name": "Profile"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "profile"
    },
    "avatar": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "files"
      ],
      "plugin": "upload",
      "required": false
    }
  }
}
